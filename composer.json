{
    "name": "doctrine/mongodb-odm-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Doctrine MongoDB Bundle",
    "keywords": ["persistence", "mongodb", "symfony"],
    "homepage": "http://www.doctrine-project.org",
    "license": "MIT",
    "authors": [
        {"name": "Bulat Shakirzyanov", "email": "mallluhuct@gmail.com"},
        {"name": "Kris Wallsmith", "email": "kris@symfony.com"},
        {"name": "Jonathan H. Wage", "email": "jonwage@gmail.com"}
    ],
    "require": {
        "php": "^7.2",
        "doctrine/annotations": "^1.2",
        "doctrine/mongodb-odm": "^2.0@alpha",
        "psr/log": "^1.0",
        "symfony/console": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/doctrine-bridge": "^3.4 || ^4.0",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/framework-bundle": "^3.4 || ^4.0",
        "symfony/options-resolver": "^3.4 || ^4.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^5.0",
        "doctrine/data-fixtures": "@dev",
        "phpunit/phpunit": "^7.3",
        "symfony/form": "^3.4 || ^4.0",
        "symfony/phpunit-bridge": "^3.4 || ^4.0",
        "symfony/yaml": "^3.4 || ^4.0"
    },
    "suggest": {
        "doctrine/data-fixtures": "Load data fixtures"
    },
    "autoload": {
        "psr-4": {
            "Doctrine\\Bundle\\MongoDBBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": { "": "Tests/DependencyInjection" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
